{
  "manifest_version": 3,
  "name": "Scrollo",
  "version": "0.1.0",
  "description": "Maintain your trello scrollbar state",
  "author": "Samip Poudel",
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.trello.com/*"],
      "js": ["script.js"]
    }
  ]
}
